{
  "name": "@WESCO-International/synergy-dna",
  "version": "1.1.32",
  "description": "Wesco Multibrand Design System (SDS)",
  "main": "index.js",
  "license": "MIT",
  "author": "Wesco",
  "publishConfig": {
    "@WESCO-International:registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesco-international/synergy-dna.git"
  },
  "bugs": {
    "url": "https://github.com/wesco-international/synergy-dna/issues"
  },
  "homepage": "https://github.com/wesco-international/synergy-dna#readme",
  "scripts": {
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "lint:css": "stylelint 'system/**/*.css' 'system/*.css'",
    "lint:fix": "stylelint --fix 'system/**/*.css' 'system/*.css'",
    "build": "node scripts/transform.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook",
    "test-storybook": "test-storybook",
    "postcss:dev": "gulp dev",
    "postcss:build": "gulp build",
    "design-system:transform": "node ./scripts/transform.mjs",
    "design-system:validate": "node ./scripts/validate-system.mjs",
    "design-system:build": "gulp build",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && npx http-server storybook --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\""
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.0",
    "@storybook/addon-a11y": "7.0.0-beta.36",
    "@storybook/addon-essentials": "7.0.0-beta.36",
    "@storybook/addon-interactions": "7.0.0-beta.36",
    "@storybook/addon-links": "7.0.0-beta.36",
    "@storybook/blocks": "7.0.0-beta.36",
    "@storybook/builder-vite": "^7.0.0-beta.36",
    "@storybook/html": "7.0.0-beta.36",
    "@storybook/html-vite": "^7.0.0-beta.36",
    "@storybook/jest": "0.0.10",
    "@storybook/test-runner": "^0.9.4",
    "@storybook/testing-library": "0.0.14-next.1",
    "concurrently": "^7.6.0",
    "cssnano": "5.1.14",
    "cz-conventional-changelog": "3.3.0",
    "gulp": "4.0.2",
    "gulp-header": "^2.0.9",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-util": "3.0.8",
    "postcss": "8.4.18",
    "postcss-import": "14.1.0",
    "postcss-mixins": "9.0.2",
    "postcss-preset-env": "7.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "19.0.2",
    "storybook": "^7.0.0-beta.36",
    "stylelint": "14.16.0",
    "style-dictionary": "3.7.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "29.0.0",
    "token-transformer": "0.0.28",
    "vite": "^4.0.4",
    "wait-on": "^7.0.1"
  }
}
